{
  "name": "crawlmattercertis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron && cross-env NODE_ENV=development electron .",
    "dev:api": "ts-node-esm ./scripts/devApiServer.ts",
    "build": "tsc -b && vite build",
    "clean": "rimraf dist-electron dist-react",
    "rebuild": "npm run clean && npm run transpile:electron && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc --project src/electron/tsconfig.json",
    "populate-dev-db": "ts-node-esm ./scripts/populateDevDb.ts",
    "convert-device-data": "ts-node-esm ./scripts/convertDeviceData.ts",
    "merge-device-data": "ts-node-esm ./scripts/mergeDeviceFiles.ts",
    "merge-matter-devices": "ts-node-esm ./scripts/mergeMatterDevFiles.ts",
    "dist:mac": "npm run transpile:electron && npm run build && electron-builder --mac --arm64",
    "dist:win": "npm run transpile:electron && npm run build && electron-builder --win --x64",
    "dist:linux": "npm run transpile:electron && npm run build && electron-builder --linux --x64"
  },
  "dependencies": {
    "@nanostores/react": "^1.0.0",
    "better-sqlite3": "^11.9.1",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "nanoid": "^5.1.5",
    "nanostores": "^1.0.1",
    "os-utils": "^0.0.14",
    "playwright": "^1.52.0",
    "playwright-chromium": "^1.52.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.3",
    "xlsx": "^0.18.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/os-utils": "^0.0.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/sqlite3": "^3.1.11",
    "@types/tailwindcss": "^3.0.11",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "express": "^4.19.2",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
